	    <script language="JavaScript">
var ndx = 0;

function displayImg( )
{
    var images = new Array( );

    images[ 1 ] = "/images/A_niger_hyphae.jpg";
    images[ 2 ] = "/images/Afumigatus1.jpg";
    images[ 3 ] = "/images/Afumigatus2.jpg";
    images[ 4 ] = "/images/Afumigatus3.jpg";
    images[ 5 ] = "/images/Anidulans_cleist.jpg ";
    images[ 6 ] = "/images/Aterreus.jpg";

    ndx = ndx + 1;
    if ( ndx < 1 || ndx >= images.length ) {
        ndx = 1;
    }
    document.getElementById( "imgseq" ).src = images[ ndx ];
    setTimeout( displayImg, 5000 );
}
setTimeout( displayImg, 5000 );
