<title>ConPlex Server</title>
<script language="JavaScript">
function MM_goToURL() {
  var i, args=MM_goToURL.arguments; document.MM_returnValue = false;
  for (i=0; i<(args.length-1); i+=2) eval(args[i]+".location='"+args[i+1]+"'");
}
</script>
<script language="JavaScript" src="/ConPlex/site_media/jquery.js"></script>
<script language="JavaScript" src="/ConPlex/site_media/jquery.progressbar.js"></script>
<script language="JavaScript" src="/ConPlex/site_media/jquery.uploadProgress.js"></script>
<script language="JavaScript">
	/* Uploading Script by WebPeace 09.08.23 */
	$(document).ready(function() {
		
		
		select_field();
		if ($.browser.safari == false) { // cross-browsing error (safari is normal mode)
			$("#uploadprogressbar").progressBar({ barImage: '/ConPlex/site_media/images/progressbg_black.gif'});
			$("#registration-form").uploadProgress();
		} 
	});
	
	function select_field() {
		if ($("#id_uFType_2:checked").val() == "pdbid") {
			$("#id_uFile").attr("disabled", "disabled");
			$("#id_uPDB").attr("disabled", ""); 
		} else {
			$("#id_uFile").attr("disabled", "");
			$("#id_uPDB").attr("disabled", "disabled");
		}
	}
	
	function input_example() {
		$("#id_uFType_2").click();
		$("#id_uPDB").val("1a2k");
		$("#id_chain1").val("AB");
		$("#id_chain2").val("C");
		$("#id_uName").val("test");
		$("#id_uEmail").val("test@test.com");
	}
	
	function alert_decoy() {
		alert("The file name of archived decoys should be:\n[ prefix ]_[ rank number ][ postfix ]\nex) docking_1.pdb");
	}
</script>
