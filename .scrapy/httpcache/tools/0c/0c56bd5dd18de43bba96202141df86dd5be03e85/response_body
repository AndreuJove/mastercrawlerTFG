<script>
function myFile() {
    var x = document.getElementById("uploadfile").value;
    if (x){
      document.getElementById("fastafileinfo").innerHTML = document.getElementById("uploadfile").files[0].name;
    }else{
      document.getElementById("fastafileinfo").innerHTML = null;
    }

}
</script>


