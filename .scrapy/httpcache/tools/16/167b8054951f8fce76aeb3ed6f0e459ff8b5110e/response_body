<script type="text/javascript">
function popup(mylink, windowname)
{
    if (! window.focus)return true;
    var href;
    if (typeof(mylink) == 'string') href=mylink;
    else href=mylink.href;
    window.open(href, windowname, 'width=500,height=200,scrollbars=yes');
    return false;
}

function example()
{
    var STR=IO("example.fasta");
    document.form1.SEQUENCE.value = STR;
}


function IO(U, V) 
{
    var X = !window.XMLHttpRequest ? new ActiveXObject('Microsoft.XMLHTTP') : new XMLHttpRequest();
    X.open(V ? 'PUT' : 'GET', U, false );
    X.setRequestHeader('Content-Type', 'text/html')
    X.send(V ? V : '');
    return X.responseText;
}

</script>

<script type="text/javascript">
function formAction (action, id)
{
    if (action == 'show')
    {
        document.getElementById("optionForm"+id).style.display= "block";
	document.getElementById("optionGraphic"+id).src="img/down_triangle.gif";
	document.getElementById("option"+id).href= "javascript:formAction('hide', "+id+")";
    }
    
    if (action == 'hide')
    {
        document.getElementById("optionForm"+id).style.display= "none";
	document.getElementById("optionGraphic"+id).src="img/right_triangle.gif";
	document.getElementById("option"+id).href= "javascript:formAction('show', "+id+")";
    }
}
</script>
