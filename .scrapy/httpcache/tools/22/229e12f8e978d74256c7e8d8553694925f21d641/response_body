   <script>

      var encrypted_password = '';

      window.onload = function () {
         if ('') {
            window.location.hash = '#' + '';
         }
      }


      function transfer_user_name (display_user_name) {
         var f_up = document.forms.user_name_and_password_form;
         f_up.user_name.value = display_user_name;
      }


      function form_submit (form_name, show_files_flag, target_search_flag,
                            user_proteins_only_flag) {
         if (form_name == 'upload_target_pdb_form'
                    && show_files_flag == 0
                    && target_search_flag != 1
                    && document.forms[form_name].target_file.value == '') {
            alert ('Please enter file to upload');
            document.forms[form_name].target_file.focus ();
         } else if (form_name == 'upload_template_pdb_form'
                    && show_files_flag == 0
                    && document.forms[form_name].template_file.value == '') {
            alert ('Please enter file to upload');
            document.forms[form_name].template_file.focus ();
         } else if (form_name == 'browse_pdb_vs_csa_form'
                    && document.forms[form_name].input_protein_name.value == '') {
            alert ('Please enter protein ID');
            document.forms[form_name].input_protein_name.focus ();
         } else {
            var f = document.forms[form_name];
            if (form_name == 'user_binding_site') {
               f.user_proteins_only_flag.value = user_proteins_only_flag;
            }
            var f_up = document.forms.user_name_and_password_form;
            var password  = f_up.password.value;
            if (password == '' || password.length >= 6) {
               var user_name = f_up.user_name.value;

               f.user_name.value = user_name;
               f.password.value  = password;
               f.encrypted_password.value = encrypted_password;
               f.show_files_flag.value = show_files_flag;
               f.target_search_flag.value = target_search_flag;
               f.submit ();
            } else {
               alert ('Password must have at least six characters')
               f_up.password.focus ();
            }
         }
      }


      function enter_sample_data (checkbox_el) {
         var textarea_el = document.getElementById ('id_binding_site_definition');

         if (checkbox_el.checked) {
            textarea_el.value =   '# Mandelate racemase representative of Enolase superfamily from Meng, et al.\n'
                                + '2mnr, enolase/5.1.2.2,LYS|HIS, A, 164, Mandelate racemase\n'
                                + '2mnr, enolase/5.1.2.2, ASP, A, 195, Mandelate racemase\n'
                                + '2mnr, enolase/5.1.2.2, GLU, A, 221, Mandelate racemase\n'
                                + '2mnr, enolase/5.1.2.2, GLU|ASP|ASN, A, 247, Mandelate racemase\n'
                                + '2mnr, enolase/5.1.2.2, HIS|LYS, A, 297, Mandelate racemase';
         } else {
            textarea_el.value = '';
         }
         var other_checkbox = document.getElementById ('example2_checkbox');
         other_checkbox.checked = false;
      }


      function enter_sample_data2 (checkbox_el) {
         var textarea_el = document.getElementById ('id_binding_site_definition');

         if (checkbox_el.checked) {
            textarea_el.value = '# Bifunctional enzyme 6-phosphofructo-2-kinase/fructose-2,6-bisphosphatase\n'
                              + '# Catalytic site 1\n'
                              + '1bif, 1/5.4.2.1, HIS, A, 256, Phosphoglycerate mutase\n'
                              + '1bif, 1/5.4.2.1, ARG, A, 305, Phosphoglycerate mutase\n'
                              + '1bif, 1/5.4.2.1, GLU, A, 325, Phosphoglycerate mutase\n'
                              + '1bif, 1/5.4.2.1, HIS, A, 390, Phosphoglycerate mutase\n'
                              + '\n'
                              + '# Catalytic site 2\n'
                              + '1bif, 2/3.1.3.46, ARG, A, 255, Fructose-2,6-bisphosphate 2-phosphatase\n'
                              + '1bif, 2/3.1.3.46, HIS|ALA, A, 256, Fructose-2,6-bisphosphate 2-phosphatase\n'
                              + '1bif, 2/3.1.3.46, ASN, A, 262, Fructose-2,6-bisphosphate 2-phosphatase\n'
                              + '1bif, 2/3.1.3.46, ARG, A, 305, Fructose-2,6-bisphosphate 2-phosphatase\n'
                              + '1bif, 2/3.1.3.46, GLU, A, 325, Fructose-2,6-bisphosphate 2-phosphatase\n'
                              + '1bif, 2/3.1.3.46, HIS|ALA, A, 390, Fructose-2,6-bisphosphate 2-phosphatase\n'
                              + '\n'
                              + '# (Additional sites/proteins may be added here)';
         } else {
            textarea_el.value = '';
         }
         var other_checkbox = document.getElementById ('example1_checkbox');
         other_checkbox.checked = false;
      }



      function delete_file (file_path) {
         var f = document.forms.delete_file_form;
         f.file_path.value = file_path;
         f.submit ();
      }


      function logout () {
         var f = document.forms.user_name_and_password_form;

         f.submit ();
      }


   </script>



</head>
