<script>
	
function which_input_flip(){


	var form = document.getElementById("biana_server_form");
	var value = undefined;

	for( i=0; i<form.which_input.length; i++ ){
		if( form.which_input[i].checked ){
			value = form.which_input[i].value;
			break;
		}
	}

	var textarea = document.getElementById("input_identifiers_textarea");
	var fileform = document.getElementById("seeds_file_"+input_file_id);

	if( value=="manual" ){ textarea.disabled=false;fileform.disabled=true; }else{textarea.disabled=true;fileform.disabled=false; }

}

</script>

