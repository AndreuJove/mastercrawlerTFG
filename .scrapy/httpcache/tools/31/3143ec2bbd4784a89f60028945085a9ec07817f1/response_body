<script>
        
function check_create_network(){


        var form = document.getElementById("biana_server_form");
	var disabled;
	
	if( form.create_network_checkbox.checked ){
		disabled = false;
	}
	else{
		disabled = true;
	}


	form.rel_types.disabled = disabled;
	form.methods.disabled = disabled;
	form.network_depth_select.disabled = disabled;
	form.add_predictions_checkbox.disabled = disabled;
	form.prediction_att.disabled = disabled;
	form.self_relations.disabled = disabled;

	if( !form.add_predictions_checkbox.checked || disabled){
		form.prediction_att.disabled = true;
	}
}


</script>


