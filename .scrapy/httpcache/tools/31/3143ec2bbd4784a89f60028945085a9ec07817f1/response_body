<script type="text/javascript">



var current_tab = 0;



var tabs = new Array("experiment_options","input_parameters","commands","output_parameters","my_experiments");



function go_to_next_tab(){



        if( current_tab == tabs.length-1 ){

                return;

        }

        current_tab += 1;

        flip_div_display(tabs[current_tab]);

        select_tab( tabs[current_tab] );

}



function go_to_previous_tab(){



        if( current_tab == 0 ){

                return;

        }

        current_tab -= 1;

        select_tab( tabs[current_tab] );

}



function select_tab(tab){



        //Remove not tabbed elements



        document.getElementById("form_display_div").style.display = "none";

        document.getElementById("submitted_experiment_tab").style.display = "none";

        document.getElementById("additional_content_tab").style.display = "none";



        if( tabs[current_tab] != tab ){

                for( var i=0; i<tabs.length; i++ ){

                        if( tabs[i]==tab ){

                                current_tab = i;

                        }

                }

        }



        replace_class( "selected_tabbed_cell", "tabbed_cell", document.getElementById("tabbed_table") );



        for( var i=0; i<tabs.length; i++ ){

                document.getElementById(tabs[i]).style.display = "none";

        }



        if( tab != undefined ){

                flip_div_display(tab);

                document.getElementById(tab+'_tab').className = "selected_tabbed_cell";

        }

}



function flip_div_display(div_id){



    div = document.getElementById(div_id);

    if(div.style.display=='none'){

        div.style.display='block';

    }

    else{

        div.style.display='none';

    }

}



function replace_class( old_class, new_class, domNode ){



        if (domNode == null) domNode = document;

        var el = new Array();

        var tags = domNode.getElementsByTagName('*');

        var tcl = " "+old_class+" ";

        for(i=0,j=0; i<tags.length; i++) {

                var test = " " + tags[i].className + " ";

                if (test.indexOf(tcl) != -1)

                        //el[j++] = tags[i];

                        tags[i].className = new_class;

        }

}



</script>










