    <script language="javascript" type="text/javascript">
        function controlGrid(cb) {
            var msg = document.getElementById('warngrid');
            msg.style.display = "none";
            if (cb.checked) {
                var grid1 = parseFloat(document.getElementById('grid1').value);
                var grid2 = parseFloat(document.getElementById('grid2').value);
                var grid3 = parseFloat(document.getElementById('grid2').value);
                if ((grid1 != NaN && grid1 < 0.5) || (grid2 != NaN && grid2 < 0.5) || (grid2 != NaN && grid2 < 0.5)) {
                    msg.style.display = "inline";
                }
            }
        }
    </script>
    <style type="text/css">
        .style1
        {
            width: 365px;
        }
