</div>
    <script type="text/javascript">
       (function($) {
  "use strict";
  
  var itemTemplate = $('.example-template2').detach(),
      editArea = $('.edit-area2'), 
      firstItem = 1,
      itemNumber = firstItem, 
      maxItems = 10;
  
  $(document).on('click', '.edit-area2 .add', function(event) {
    var item = itemTemplate.clone();
    item.find('[name]').attr('name', function() {
//      return $(this).attr('name') + '_' + itemNumber;
      return $(this).attr('name');
      });
    if(itemNumber <= maxItems) {
    	++itemNumber;
    	item.appendTo(editArea);
	} else {
		alert("Maximum number of rows is 10");
    }
  });
  
  $(document).on('click', '.edit-area2 .rem', function(event) {
	  editArea.children('.example-template2').last().remove();
	  if(itemNumber > firstItem) { 
	  	--itemNumber;
	}
  });
  
  $(document).on('click', '.edit-area2 .del', function(event) {
    var target = $(event.target),
        row = target.closest('.example-template2');
	row.remove();
	  if(itemNumber > firstItem) { 
	  	--itemNumber;
	}

  });
}(jQuery));
    </script></form><br>
 
  </div>
