	function nullFn() {}
	
	var nFields = '';

	// -------------------------------------------
	//	butDAVIDhelp
	// -------------------------------------------
	function butDAVIDhelp() {
		window.open('nprot.2008.211.pdf','DAVID_Help');
	}
	
	// -------------------------------------------
	//	butAnnotation
	// -------------------------------------------
	var URIchange = '';
	function butAnnotation(species,idList) {
		if ( idList == '' ) {
			alert( 'No '+species+' orthologues found.' );
		} else {
			//alert( idList );
			URIchange = ( URIchange == '' ) ? '?' : '';		// workaround to change consecutive URIs (see https://bugs.webkit.org/show_bug.cgi?id=28633 )
			document.davidForm.action = davidURL + URIchange;
			document.davidForm.ids.value = idList;
			document.davidForm.submit();
		}
	}
        	
	// -------------------------------------------
	//	downloadCSV
	// -------------------------------------------
	function downloadCSV() {
		var action = document.searchForm.action;
		document.searchForm.action = '/cgi-bin/BB/data.py';
		document.searchForm.type.value = 'csv';
		document.searchForm.submit();
		document.searchForm.action = action;
	}

	// -------------------------------------------
	//	initPage
	// -------------------------------------------
	function initPage() {
		hide('searchHide','');
		hide('resultsHide','none');			// --- SET BUTTON/CHECKBOX STATES CORRECTLY ON RELOAD ---
		idInputBUT();
		orthoBUT();
	}

	</script>
