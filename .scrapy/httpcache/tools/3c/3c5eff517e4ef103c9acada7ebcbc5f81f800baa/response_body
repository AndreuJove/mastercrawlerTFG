  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.0/jquery-confirm.min.js"></script> 
  <script>
  if ($.trim( $('#leftsidecontent').text() ) == "")
	    $('#leftsidecontent').remove();
  </script> 
  <script>
    var isChangedSeq = false;
	function resetInput() {	
		$("textarea#sequence").val("");
		$("textarea#asequence").val("");
		$("textarea#bsequence").val("");
		isChangedSeq = false;
	}
	
	$(document).ready(function(){
      
		$("#exampleSeq").click(function(e){
			e.preventDefault();
			if ("spp" == getRequiredSeqType()) {
				$("textarea#asequence").val(sequenceProteinA);
				$("textarea#bsequence").val(sequenceProteinB);
			} else if ("spd" == getRequiredSeqType()) {
				$("textarea#asequence").val(sequenceDNAA);
				$("textarea#bsequence").val(sequenceDNAB);
			} else if ("ssx" == getRequiredSeqType() ) {
				$("textarea#asequence").val(ssp);
				$("textarea#bsequence").val(ssd);
			} else {
				$("textarea#sequence").val(eval(getRequiredSeqType()));	
			}
			isChangedSeq = false;
			
		});
		
		$("#seeMoreExample").click(function(e){
			e.preventDefault();
			window.open(getSequenceSampleUrl());
		});
	
		$("#exampleSeqTest").change(function(){		
			$("textarea#sequence").val(eval($("#exampleSeqTest").val()));
		});
		
		$("#clearSequence").click(function(e){
			e.preventDefault();
			
			if (isChangedSeq == false) {
				resetInput();
				return;
			}
			
			$.confirm({
				title: 'Clear Sequence',
				content: 'The input was changed. \n It is going to be clear. ',
				buttons: {
					confirm: function () {
						//$.alert('Confirmed!');
						resetInput();
					},
					cancel: function () {
						//$.alert('Canceled!');
					},
					somethingElse: {
						text: 'See more example sequences',
						btnClass: 'btn-blue',
						keys: ['enter', 'shift'],
						action: function(){
							window.open(getSequenceSampleUrl()); 
						}
					}
				}
			});
        });

		
		$("textarea#sequence").change(function(){
			isChangedSeq = true;
		});

		$("textarea#asequence").change(function(){
			isChangedSeq = true;
		});

		$("textarea#bsequence").change(function(){
			isChangedSeq = true;
		});

	});	
	
  </script> 
  <script src="../../common/assets/js/form/common.js" type="text/javascript"></script> 
