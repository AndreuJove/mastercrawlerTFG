<script type="text/javascript">
/* <![CDATA[ */
    
    		
	/* enable/disables fields that depend on the 'changedElement' */
	function setValid( changedElement ) {
		if (changedElement.name == "alignType") {
			var diffbox = document.getElementById("turnOffAlign");
			var alignNorm = document.getElementById("alignMode_normal");
			var alignProb = document.getElementById("alignMode_probabilistic");
			var consField = document.getElementById("consistencyTransform");
			var alifoldConsensusDP = document.getElementById( "alifoldConsensusDP" );
			var ignoreConstraints = document.getElementById( "ignoreConstraints" );

			if (changedElement.value == "Global") {
				alignProb.disabled = false;
				if (alignProb.checked) {
					consField.disabled = false;
					alifoldConsensusDP.checked = false;
					ignoreConstraints.checked = false;
				}

				diffbox.disabled = false;
				diffbox.checked = false;
				setFieldToMinusOne( diffbox, "maxDiffAlign" );
			} else if (changedElement.value == "Local") {
				alignProb.disabled = true;
				alignNorm.checked = true;
				consField.disabled = true;
				consField.checked = false;
				alifoldConsensusDP.checked = true;

				diffbox.disabled = true;
				diffbox.checked = true;
				setFieldToMinusOne( diffbox, "maxDiffAlign" );
			}
		} 
	}
	
	/* enables/disables the element 'fieldId' based on the value of checkbox element 'checkboxId' */
	function disableField( checkboxId, fieldId ) {
		var checkbox = document.getElementById( checkboxId );
		var field = document.getElementById( fieldId );
		if( checkbox.checked )
			field.disabled = true;
		else
			field.disabled = false;
	}
	
	
	function setConsistency(check) {
		var consField = document.getElementById( "consistencyTransform" );
		var alifoldConsensusDP = document.getElementById( "alifoldConsensusDP" );
		var ignoreConstraints = document.getElementById( "ignoreConstraints" );
		if (check.id == "alignMode_probabilistic") {
			  // enable consistency input and make selected by default
			consField.disabled = false;
			consField.checked = true;
			alifoldConsensusDP.checked = false;
			ignoreConstraints.checked = true;
		} else {
			  // disable consistency stuff
			consField.disabled = true;
			consField.checked = false;
			alifoldConsensusDP.checked = true;
			ignoreConstraints.checked = false;
		}
	}
	
	
	/* enables/disables the element 'fieldId' and sets its value to '-1' based on the value of 'checkbox' */
	function setFieldToMinusOne( checkbox, fieldId ) {
		var field = document.getElementById( fieldId );
		if( checkbox.checked ) {
			// store field value
			document.getElementById( fieldId+"Tmp" ).innerHTML = field.value;
			// and set to minus one
			field.value = "-1";
		} else {
			// reset field value
			field.value = document.getElementById( fieldId+"Tmp" ).innerHTML;
		}
		// disable field if needed
		disableFieldIfMinusOne( fieldId );
	}
	
	/* enables/disables the element 'fieldId' and sets its value to '-1' based on the value of 'checkbox' */
	function disableFieldIfMinusOne( fieldId ) {
		var field = document.getElementById( fieldId );
		field.readOnly = field.value == "-1";
	}
	
	
	function clearAllDotPlotFiles(){
		$('input:file').MultiFile('reset');
		document.getElementById( "dotPlotFiles_wrap" ).style.color = "#545454";
	}
	
	function showOrHideMessage(){
		var dotPlotFiles = document.getElementsByClassName( "MultiFile-label" );
		var desiredDiv = document.getElementById( "dotPlotFiles_wrap" );
		if( dotPlotFiles.length > 0 )
			desiredDiv.style.color = "transparent";
		else
			desiredDiv.style.color = "#545454";
	}
	
	
/* ]]> */
</script>
       


