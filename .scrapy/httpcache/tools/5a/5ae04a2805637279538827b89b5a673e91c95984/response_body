	<section class="heading-hero">
      <div class="row">
        <div class="small-12 columns text-center">
          <h1>Column Selector</h1>
        </div>
      </div>
    </section>
    <section>
      <div class="row">
        <div class="small-12 medium-12 columns">
          <nav aria-label="You are here:" role="navigation">
            <ul class="breadcrumbs">
              <li><a href="/resources/">Resources</a></li> <li><a href="/resources/freeware/">Chemistry Software</a></li> <li><span class="show-for-sr">Current: </span><a href="/resources/freeware/colsel/">ACD/Column Selector</a></li>            </ul>
          </nav>
