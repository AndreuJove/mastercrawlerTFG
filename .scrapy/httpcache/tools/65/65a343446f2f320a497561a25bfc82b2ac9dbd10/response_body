	{popupWindow("helpUpload.html","windowCHAIN",270,420,150,210);}
	function windowCHAIN()
	{popupWindow("helpChain.html","windowCHAIN",270,420,150,210);}
	function windowPARAMETERS()
	{popupWindow("helpParameters.html","windowPARAMETERS",350,420,150,210);}
	function windowALIGNPARAMETERS()
	{popupWindow("helpAlignParameters.html","windowALIGNPARAMETERS",440,250,150,210);}
	function windowBOLTZMANN()   
	{popupWindow("helpBoltzmann.html","windowBOLTZMANN",500,250,150,210);}
	function windowRNADIHEDRALS()
