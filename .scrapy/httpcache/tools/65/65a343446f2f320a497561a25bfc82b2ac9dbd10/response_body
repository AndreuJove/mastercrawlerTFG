	function popupWindow(url,title,w,h,l,t)
	{window.open(url,title,"toolbar=no,directories=no,menubar=no,status=no,resizeable=no,scrollbars=yes,width="+w+",height="+h+",left="+l+",top="+t);}             	function windowUPLOAD()   {popupWindow("helpUpload.html","windowUPLOAD",500,250,150,210);}
