	function windowFragmented()
	{popupWindow("helpFragmented.html","windowFRAGMENTED",250,600,150,210);}

function selectTab(tabIndex, name) {
	tabs = document.getElementsByName(name)
	tab = tabs[tabIndex]
	tab.checked = true
	groupname = tab.getAttribute('groupname')
	sections = document.getElementsByClassName(groupname)
	for (var i=0 ; i < sections.length ; i++)
		sections[i].style.display = 'none'
	sections[tab.value].style.display = 'block'	
}
