<script language="JavaScript">
<!--


  loadxmap = '';

  function SetMapUpload ( sel, suffix )
  {
    var a = document.getElementById ('upload_map_input' + suffix);
    if ( sel.options[5].selected )
    { a.innerHTML = a.innerHTML.substring (4, a.innerHTML.length-3);
      if ( loadxmap = '' ) loadxmap = a.innerHTML;
    } else
    { a.innerHTML = '<!--' + loadxmap + '-->';
    }
  }


var BetaDetectorSelect = new Array ();
    BetaDetectorSelect[""] = "";
    BetaDetectorSelect["_2"] = "";
    BetaDetectorForDatabase = " SheeP based on DSSP ";

function SetInput ( suffix, sel )
{
  if ( sel.value == "pdb90" )
  { document.getElementById("input" + suffix).innerHTML = "";
    document.getElementById("beta_detector" + suffix).innerHTML = BetaDetectorForDatabase;
    return;
  }

  document.getElementById("input" + suffix).innerHTML=InputForms[sel.selectedIndex].replace(/=SUFFIX=/g, suffix);
  document.getElementById("beta_detector" + suffix).innerHTML = BetaDetectorSelect[suffix];

  }

function PrintInputForm ( header, suffix, allow_databases )
{
  document.write ("<h2>" + header + "</h2>" );
  document.write ("<font size=-1><a href='proton.php?subproj=archip&page=help&data=interface_startnewjob' target='_new'>help</a></font>:" );
  document.write ("<select name='source" + suffix + "' id='source" + suffix + "' onChange='SetInput(\"" + suffix + "\", this);'>");
  document.write ("  <option " + ( allow_databases ? "" : "selected" ) + " value='uploadfile'>Upload your local file (in PDB format)</option>" );
  document.write ("  <option value='pdbcode'>Enter PDB code</option>" );                
  document.write ("  <option value='example'>Use example (select from list)</option>" );
  if ( allow_databases )
    document.write ("  <option selected value='pdb90'>Protein Data Bank (set of records with less than 90% identity to each other)</option>" );
  document.write ("</select>" );
  document.write ("<a id='input" + suffix + "'>" );
  if ( ! allow_databases )
    document.write ( InputForms[0].replace ( /=SUFFIX=/g, suffix ) );
  document.write ( "</a></p>" );

  
  document.write ('<a style="font-size: 25; text-decoration: underline;">&beta;-sheets detector:</a>' );
  document.write ('<a id="beta_detector' + suffix + '">' );

  BetaDetectorSelect[suffix] = '<select name="sse_detect' + suffix + '" onChange="SetMapUpload(this, \'' + suffix + '\');">';
  BetaDetectorSelect[suffix] += ' <option value="dsspmod">SheeP based on DSSP (recommended)</option>';
  BetaDetectorSelect[suffix] += ' <option value="stridemod">SheeP based on STRIDE</option>';
  BetaDetectorSelect[suffix] += ' <option value="dssp">Original DSSP (SheeP will build maps without modifications)</option>';
  BetaDetectorSelect[suffix] += ' <option value="stride">Original STRIDE (SheeP will build maps without modifications)</option>';
  BetaDetectorSelect[suffix] += ' <option value="makessp">SheeP based on MakeSSP (recommended for C-alpha only files)</option>';

  BetaDetectorSelect[suffix] += "<option value='upload_map'>Upload your map in XMAP format</option>";  BetaDetectorSelect[suffix] += '</select>';

  if ( ! allow_databases) 
  { document.write ( BetaDetectorSelect[suffix] );
  } else
  { document.write ( BetaDetectorForDatabase );
  }

  document.write ('</a><font size=-1><a href="proton.php?subproj=archip&page=help&data=interface_makemaps" target="_new">help</a></font>' );
  document.write ('<p>');

  document.write ('<a id="upload_map_input' + suffix + '"><!--<input type=file name="xmapfile' + suffix + '" id="xmapfile' + suffix + '">--></a>' );
  
}

  PrintInputForm ( '', '', false );

//-->
</script>

