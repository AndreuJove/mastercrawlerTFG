<script>
function toFullTextSearch(){
	var full_text_keyword = document.getElementById("quick_full_text_search_text").value;
	if(full_text_keyword.length > 2){
		window.location.href = path+'/module/search/text_search.jsp?quickSearch='+full_text_keyword;
	}else{
		alert('The KeyWord should contain at least 2 characters.');
	}
};
$(document).ready(function(){
    $('#quick_full_text_search_text').bind('keyup', function(event){
        if (event.keyCode=="13"){
        	toFullTextSearch();
        }
    });
});
//menu slides
$(document).ready(function(){
  $('li.mainlevel').mouseenter(function(){
  $(this).find('ul').show(1);//you can give it a speed
  });
  $('li.mainlevel').mouseleave(function(){
  $(this).find('ul').hide(1);
  });
});
$.ajax({
	url: path + '/AccessRecord',
	type: 'post',
	data:{page: 'Mainpage'},
	dataType:'json',
	success:function(data){}	
});
//
</script>
