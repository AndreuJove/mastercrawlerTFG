						<script>
							//<![CDATA[
							var organism = null;

							$(window).load(function() {
								$.ajax({
									dataType: "json",
									url: '/data/atlas/typeahead.json',
									type: 'GET',
								}).done(function (json) {
									$("#global-search").typeahead({
										source: json,
										autoSelect: true,
										displayText: function (item) {
											if("gene" in item) return item["gene"];
											if("mirna" in item) return item["mirna"];
										}
									});
								});
							});

							$("#global-search").keyup(function(event){
								if(event.keyCode == 13){
									var current = $("#global-search").typeahead("getActive");
									if(current["mirna"] !== undefined){
										window.location = "https://mpd.bioinf.uni-sb.de/mirna.html?mirna=" + current["mirna"];
									} else if(current["gene"] !== undefined) {
										window.location = $('#genetrail2_url').val() + "/genetrail_atlas.html?gene=" + current["gene"] + "&organism=" + $("#atlas-organism-select").val();
									}
								}
							});
							//]]>
						</script>
						
