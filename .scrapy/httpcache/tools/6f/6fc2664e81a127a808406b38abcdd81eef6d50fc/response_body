<script language="javascript">
function showDiv(divId){
	document.getElementById(divId).style.display = 'block';
}

function opacMouseOver(item, value){
	item.style.opacity = value;
}

function selectHover(ID, state){
var imgObj = document.getElementById(ID);
	if(state){
		imgObj.style.filter       = "alpha(opacity=70)";
		imgObj.style.MozOpacity   = 0.7;
		imgObj.style.opacity      = 0.7;
		imgObj.style.KhtmlOpacity = 0.7;
	}
	else{
		imgObj.style.filter       = "alpha(opacity=100)";
		imgObj.style.MozOpacity   = 1;
		imgObj.style.opacity      = 1;
		imgObj.style.KhtmlOpacity = 1;
	}
}

</script>
<script type="text/javascript" src="gans.js"></script>

