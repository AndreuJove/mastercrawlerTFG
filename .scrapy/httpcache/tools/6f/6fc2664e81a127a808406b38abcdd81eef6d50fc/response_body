<script language="javascript">
var animationBoxes ={}

function animateDiv(listName, elementId, animate){
	animate = animate || false;
	if (animate==false)
		stopTimer(listName)

	var currentId = animationBoxes[listName].selected;
	var oldId = listName + "_" + currentId;
	var newId = listName + "_" + elementId;
	var oldLinkId = listName + "_Link_" + currentId;
	var newLinkId = listName + "_Link_" + elementId;

	document.getElementById(oldId).className ="";
	document.getElementById(newId).className ="opaque";

	document.getElementById(oldLinkId).innerHTML ="&nbsp;<img src=\"img/radiobox.png\">&nbsp;";
	document.getElementById(newLinkId).innerHTML ="&nbsp;<img src=\"img/radioboxS.png\">&nbsp;";

	animationBoxes[listName].selected = elementId;
}


function stopTimer(boxname){
	clearInterval(animationBoxes[boxname].timer);
}

function swichDiv(listName, elementId){
	var currentId = animationBoxes[listName].selected;
	var oldId = listName + "_" + currentId;
	var newId = listName + "_" + elementId;
	document.getElementById(oldId).className = "";
	document.getElementById(newId).className = "opaque";
	animationBoxes[listName].selected = elementId;
}

function animateNext(listName, animate){
	var currentId = animationBoxes[listName].selected;
	var newNumber = animationBoxes[listName].pages.indexOf(currentId);
	newNumber = newNumber + 1;
	if (newNumber >= animationBoxes[listName].pages.length)
		newNumber=0;
	animateDiv(listName, animationBoxes[listName].pages[newNumber], animate);
}

function animatePrevious(listName, animate){
	var currentId = animationBoxes[listName].selected;
	var newNumber = animationBoxes[listName].pages.indexOf(currentId);
	newNumber = newNumber -1;
	if (newNumber < 0)
		newNumber = animationBoxes[listName].pages.length-1;
	animateDiv(listName, animationBoxes[listName].pages[newNumber], animate);
}


</script>

