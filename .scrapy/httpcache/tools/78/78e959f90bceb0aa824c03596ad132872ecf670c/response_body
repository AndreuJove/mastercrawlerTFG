   <script type="text/javascript">
        $(document).ready(function() {
            var $foo = $('#nach_oben');
            $foo.hide();
            var $win = $(window).scroll(function() {
            if($win.scrollTop() > 400) {
                if(!$foo.is(':visible')) {
                    $foo.stop(true, true).fadeIn();
                }
            } else {
                if(!$foo.is(':animated')) {
                    $foo.stop(true, true).fadeOut();
                }
            }
            });

        });   
        
        function changeClass(MyElement)
        {
            if ( document.getElementById(MyElement).classList.contains('zoom') ){
                    document.getElementById(MyElement).classList.remove('zoom');
            } else{
                document.getElementById(MyElement).classList.add('zoom');
            }
        }
    </script>


 
