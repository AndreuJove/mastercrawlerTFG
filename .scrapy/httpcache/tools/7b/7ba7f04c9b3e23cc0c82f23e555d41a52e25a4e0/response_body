<script>
function check(){
		if($("#input_seq").val()=="" || $("#input_seq").val()==null){
			window.alert("Please input your sequence.");  
			return false; 
		}else{
		  var temp=$("#input_seq").val().split("\n");
		  if(temp.length>100000){
			window.alert("Please input no more than 100000 lines at one time. Or you can try 'Batch Model' to upload viax file.");  
			return false; 		
		  }
		  var seq_tag="";
		  for(var i=0;i<temp.length;i++){
			if(temp[i].substr(0,1)=="#" || temp[i].substr(0,1)==" " || temp[i]==""){
				continue;
			}else{
				var pattern1 = /^(\S+\t){3}(\d+\t){2}\S+\t(\+|\-|\.)\t(0|1|2|\.)\t(.+)$/;
				var pattern2 = /^[\S]*[\s]+[0-9]+[\s]+[0-9]+[\s]+[\S]+[\s]+[0-9\.]+[\s]+[+-].*/;
				if(temp[i].substr(0,1)==">"){
					seq_tag="FASTA";
					break;
				}else if(temp[i].match(pattern1)){
					seq_tag="GTF";
					break;
				}else if(temp[i].match(pattern2)){
					seq_tag="BED";
					break;					
				}else{
					seq_tag="unknown";
				}
			}
		  }
		  if(seq_tag=="FASTA"){
			document.getElementById('main_input_form').submit();
			$('#submit_button').val("Running, please wait for a moment...");			  
		  }else if(seq_tag=="GTF"||seq_tag=="BED"){
			  //select a genome version 
			  $('#myModal').modal('show');		  
		  }else if(seq_tag=="unknown"){
			window.alert("Your input isn't in fasta, gtf or bed format. If your input is in gtf or bed format, please make sure echo column is separated by \"tab\". Usually \"tab\" cannot be typed directly in a html web page. You can edit the format with \"tab\" in word, then copy and paste it here.");  
			return false; 
		  }
		}
}		

function run_gtf_bed(){
		document.getElementById('main_input_form').submit();
		$('#submit_button').val("Running, please wait for a moment...");	
}
</script>



