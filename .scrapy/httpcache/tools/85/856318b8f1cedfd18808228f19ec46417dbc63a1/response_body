           <script type="text/javascript">
		//script to make all panels equal in height
                jQuery.noConflict();


                function setPanelHeight() {
			jQuery('.same-height').css("height", "auto");//set size back to auto
			var maxHeight = 0;

			//determine panel with the biggest height
			jQuery('.same-height').each(function(i, obj) {
    				if (jQuery(this).height() > maxHeight) {
					maxHeight = jQuery(this).height();
				}
			});

			//set all heights
                        jQuery('.same-height').height(maxHeight);

                }
                jQuery(document).ready(function ( $ ) {
                        setPanelHeight();
                });
                jQuery(window).resize(function ( $ ) {
                        setPanelHeight();
                });
                



           </script>    


	<script type='text/javascript'>
		//<![CDATA[
		function more_info(link_div_id){			
			var t	= document.getElementById(link_div_id);
			if(t.style.display=="" || t.style.display=="none"){
				$("#"+link_div_id).slideDown();
			}
			else{
				$("#"+link_div_id).slideUp();
			}
		}		
		//]]>

		
	</script>

