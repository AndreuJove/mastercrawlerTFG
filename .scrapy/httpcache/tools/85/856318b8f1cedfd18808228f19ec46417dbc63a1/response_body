	   <script type="text/javascript">
		//<![CDATA[
		jQuery.noConflict();
		function moveFooter() {
			jQuery('#content').css('height', "auto");
			jQuery('#content').css('min-height', 500);
			var pageHeight = jQuery('#header').height() + jQuery('#footer').height() + jQuery('#content').height();
			//window.alert( jQuery(window).height()  + " " + pageHeight );
			var extra = jQuery(window).height() - jQuery('#header').height() - jQuery('#footer').height() + 10; //10px overlap with navbar needs to be accounted for
     			if (jQuery(window).height() > pageHeight && extra > 500)
        		{
       				jQuery('#content').height(extra);
				jQuery('#content').css('height', extra);
				jQuery('#content').css('min-height', extra);
        		} 		

		}
		jQuery(document).ready(function ( $ ) {
			moveFooter();
		});
		jQuery(document).click(function( $ ) {
			moveFooter();
		});
		jQuery(window).resize(function ( $ ) {
			moveFooter();
		});
		

		window.setInterval(function(){
			moveFooter();
		}, 250);

		//]]>
	   </script>	


