<style type="text/css" title="currentStyle">@import "i.css";</style>
</head>
<script type='text/javascript'>
function ClearPicArea()
{
	var s=document.getElementById('PicArea');
	s.innerHTML='';
}
function ClearInfoArea()
{
	var s=document.getElementById('InfoArea');
	s.innerHTML='';
}
function ClearArea()
{
	var s=document.getElementById('Area');
	s.innerHTML='';
}
function PicMsg(Msg)
{
	var s=document.getElementById('PicArea');
	s.innerHTML+=Msg;
}
function InfoMsg(Msg)
{
	var s=document.getElementById('InfoArea');
	s.innerHTML+=Msg;
}
function LoadFile(FileName)
{
	var s;
	var LoadFileRequest;
	if (window.ActiveXObject)	// IE
		LoadFileRequest = new ActiveXObject("Msxml2.XMLHTTP");
	else if(window.XMLHttpRequest)		// Mozilla, Safari, ...
		LoadFileRequest = new XMLHttpRequest();
	else{}
	if(LoadFileRequest)
	{
		LoadFileRequest.onreadystatechange=function()
		{
			if (LoadFileRequest.readyState==4)
			{
				s=document.getElementById('Area');
				s.innerHTML=LoadFileRequest.responseText;
			}
		};
		LoadFileRequest.open('GET',FileName,true);
		LoadFileRequest.send(null);
	}
	else
	{
		s=document.getElementById('Area');
		s.innerHTML='Error';
	}
}
function LoadFileClearPicArea(FileName)
{
	var s;
	var LoadFileRequest;
	if (window.ActiveXObject)	// IE
		LoadFileRequest = new ActiveXObject("Msxml2.XMLHTTP");
	else if(window.XMLHttpRequest)		// Mozilla, Safari, ...
		LoadFileRequest = new XMLHttpRequest();
	else{}
	
	if(LoadFileRequest)
	{
		LoadFileRequest.onreadystatechange=function()
		{
			if (LoadFileRequest.readyState==4)
			{
				s=document.getElementById('Area');
				s.innerHTML=LoadFileRequest.responseText;
				ClearPicArea();
			}
		};
		LoadFileRequest.open('GET',FileName,true);
		LoadFileRequest.send(null);
	}
	else
	{
		s=document.getElementById('Area');
		s.innerHTML='Error';
	}
}
</script>

