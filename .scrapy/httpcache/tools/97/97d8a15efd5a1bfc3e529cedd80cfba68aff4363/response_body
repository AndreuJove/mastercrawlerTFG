<script language="Javascript">
	for (var i = 0; i < Programs.length; i++) {
		document.write("<a name='"+Programs[i].number+"'></a><DIV align=left><TABLE class=white-box cellspacing=0 cellPadding=0 width=100% border=0><TBODY><TR height=20><TD class='box-top-left'></TD><TD class='box-top'></TD><TD class='box-top-right'></TD></TR>");
		document.write("<TR><TD class='box-left'></TD><TD><DIV class='box-subtitle'>"+Programs[i].title+"</DIV></TD><TD class='box-right'></TD></TR>");
		document.write("<TR><TD class='box-left'></TD><td><DIV class='box-body2'>"+Programs[i].description+"</div></TD><TD class='box-right'></TD></TR>");

		var  k = -1;
		if (Programs[i].hasPapers) {
			if (Programs[i].PaperArray.length > 1) document.write("<TR><TD class='box-left'></TD><td><DIV class='box-body2'><b><i>from the papers: </i></b></div></TD><TD class='box-right'></TD></TR>");
			else document.write("<TR><TD class='box-left'></TD><td><DIV class='box-body2'><b><i>from the paper: </i></b></div></TD><TD class='box-right'></TD></TR>");
			for (var j = 0; j < Programs[i].PaperArray.length; j++) {
				k = Programs[i].PaperArray[j]-1;
				if (k < 0 || k >= Papers.length) break;
				document.write("<TR><TD class='box-left'></TD><td><div class='box-innards2'>"+Papers[k].title+"</div><div class='box-innards'>"+Papers[k].authors+", <i>"+Papers[k].journal+"</i> ("+Papers[k].year+") ");
				if (Papers[k].volume) {
					document.write(Papers[k].volume);
					if (Papers[k].issue) document.write("("+Papers[k].issue+")");
					document.write(": ");
					}
				document.write(Papers[k].pages+"</DIV></TD><TD class='box-right'></TD></TR>");
				document.write("<TR><TD class='box-left'></TD><td align='right'><TABLE class='button' cellspacing='0' cellPadding='0' width='auto' border='0'><TBODY><TR><td class='box-body2'><i>For more about this paper, click&nbsp;</i></td><TD class='button-left'></TD><TD class='button-fill' nowrap><A href='publications.htm#"+Papers[k].number+"'>");
				document.write("<DIV class='button-title' align='center'>Details</DIV></A></TD><TD class='button-right'></TD></TR></TBODY></TABLE></A></TD><TD class='box-right'></TD></TR>");
				if (Papers[k].hasData) {
					document.write("<TR><TD class='box-left'></TD><td><DIV class='box-body2'><br><i>Data items associated with this paper:</i></div></TD><TD class='box-right'></TD></TR>");
					var  kk = -1;
					for (var jj = 0; jj < Papers[k].DataArray.length; jj++) {
						kk = Papers[k].DataArray[jj]-1;
						if (kk < 0 || kk >= DataItems.length) break;
						document.write("<TR><TD class='box-left'></TD><td><DIV class='box-innards2'>"+DataItems[kk].title+"</DIV><DIV class='box-innards'>"+DataItems[kk].description+"</DIV>");
						document.write("<p align='right'><A href='data.htm#"+DataItems[kk].number+"'>");
						document.write("<TABLE class='button' cellspacing='0' cellPadding='0' width='auto' border='0'><TBODY><TR><td class='box-body2'><i>To download and to get instructions for use, click&nbsp;</i></td><TD class='button-left'></TD><TD class='button-fill' nowrap><A href='data.htm#"+DataItems[kk].number+"'>");
						document.write("<DIV class='button-title' align='center'>Details</DIV></A></TD><TD class='button-right'></TD></TR></TBODY></TABLE></A>");
						}
					document.write("</div></TD><TD class='box-right'></TD></TR>");
					}
				} //end of for
			} //end of if
		document.write("<TR><TD class='box-left'></TD><td><DIV class='box-body2'>"+Programs[i].instructions+"</div></TD><TD class='box-right'></TD></TR>");
		document.write("<TR><TD class='box-left'></TD><td><h5>Download</h5>");
		if (Programs[i].forDOS) document.write("<DIV class='box-body2'>For Windows/DOS: <a href='"+Programs[i].DOSURL+"'>Download Now</a></div>");
		if (Programs[i].forUNIX) document.write("<DIV class='box-body2'>For UNIX/Linux: <a href='"+Programs[i].UNIXURL+"'>Download Now</a></div>");
		if (Programs[i].forMAC) document.write("<DIV class='box-body2'>For MacOSX: <a href='"+Programs[i].MACURL+"'>Download Now</a></div>");

		document.write("</TD><TD class='box-right'></TD></TR>");

		document.write("</TD><TD class='box-right'></TD></TR><TR height=20><TD class=box-bottom-left></TD><TD class=box-bottom></TD><TD class=box-bottom-right></TD></TR></TBODY></TABLE></DIV>");
		}
</script>

</TD></TR></TBODY></TABLE>
