            <script>
    AJS.toInit(function() {
        // disable Confluence UI elements
        if (AJS.params.remoteUser == '') {
            AJS.$('#navigation').hide();
            AJS.$('#labels-section').hide();
            AJS.$('#action-menu-link').hide();
            AJS.$('#comments-section').hide();
            AJS.$('#title-text').hide();
            AJS.$('.page-metadata').hide();
            AJS.$('#page-metadata-banner').hide();
        }

        // open external links in their own tab/window (#SCA is for Mouse Spinal Cord, i.e. Spinal Cord App)
        AJS.$('.external-link, #SCA').attr('target', '_blank');
    });
</script>
<style>
    #breadcrumbs {
        display: none;
    }
</style>

