<script type="text/javascript">
dojo.ready(function(){
    /*set up data store*/
    var data = {
      identifier: 'id',
      items: []
    };
    var data_list = [
      { col1: "canEvolve", col2: true, col3: 'Analysis Results', col4: 1},
      { col1: "canEvolve", col2: true, col3: 'Analysis Results', col4: 2},
      { col1: "canEvolve", col2: true, col3: 'Analysis Results', col4: 3}
    ];
    var rows = 12;
    for(var i=0, l=data_list.length; i<rows; i++){
      data.items.push(dojo.mixin({ id: i+1 }, data_list[i%l]));
    }
    var store = new dojo.data.ItemFileWriteStore({data: data});

    /*set up layout*/
    var layout = [[
      {'name': 'canEvolve', 'field': 'id', 'width': '20%'},
      {'name': 'Source', 'field': 'col2', 'width': '20%'},
      {'name': 'Page', 'field': 'col3', 'width': '20%'},
      {'name': 'Order', 'field': 'col4', 'width': '20%'}
    ]];

    /*create a new grid:*/
    var grid = new dojox.grid.EnhancedGrid({
        id: 'grid',
		plugins: {indirectSelection: true,exporter: true},
		selectionMode: "single",
        store: store,
        structure: layout,
        rowSelector: '0px'},
      document.createElement('div'));

    var gridcn = new dojox.grid.EnhancedGrid({
        id: 'gridcn',
		plugins: {indirectSelection: true,exporter: true},
		selectionMode: "single",
        store: store,
        structure: layout,
        rowSelector: '0px'},
      document.createElement('div'));

    var gridmut = new dojox.grid.EnhancedGrid({
        id: 'gridmut',
                plugins: {indirectSelection: true,exporter: true},
                selectionMode: "single",
        store: store,
        structure: layout,
        rowSelector: '0px'},
      document.createElement('div'));

    var gridcngeall = new dojox.grid.EnhancedGrid({
        id: 'gridcngeall',
                plugins: {indirectSelection: true,exporter: true},
                selectionMode: "single",
        store: store,
        structure: layout,
        rowSelector: '0px'},
      document.createElement('div'));

    var gridcngeselect = new dojox.grid.EnhancedGrid({
        id: 'gridcngeselect',
                plugins: {indirectSelection: true,exporter: true},
                selectionMode: "single",
        store: store,
        structure: layout,
        rowSelector: '0px'},
      document.createElement('div'));

    var griddiffge = new dojox.grid.EnhancedGrid({
        id: 'griddiffge',
                plugins: {indirectSelection: true,exporter: true},
                selectionMode: "single",
        store: store,
        structure: layout,
        rowSelector: '0px'},
      document.createElement('div'));

    var gridsurv = new dojox.grid.EnhancedGrid({
        id: 'gridsurv',
                plugins: {indirectSelection: true,exporter: true},
                selectionMode: "single",
        store: store,
        structure: layout,
        rowSelector: '0px'},
      document.createElement('div'));

    var gridcmpres = new dojox.grid.EnhancedGrid({
        id: 'gridcmpres',
                plugins: {indirectSelection: true,exporter: true},
                selectionMode: "single",
        store: store,
        structure: layout,
        rowSelector: '0px'},
      document.createElement('div'));

    //var gridcmpcolors = new dojox.grid.EnhancedGrid({
    //    id: 'gridcmpcolors',
    //            plugins: {indirectSelection: true,exporter: true},
    //            selectionMode: "single",
    //    store: store,
    //    structure: layout,
    //    rowSelector: '0px'},
    //  document.createElement('div'));

    /*append the new grid to the div*/
    dojo.byId("gridDiv").appendChild(grid.domNode);
    dojo.byId("gridCopyNumber").appendChild(gridcn.domNode);
    dojo.byId("gridMutation").appendChild(gridmut.domNode);
    dojo.byId("gridCopyGEall").appendChild(gridcngeall.domNode);
    dojo.byId("gridCopyGEselect").appendChild(gridcngeselect.domNode);
    dojo.byId("gridDGE").appendChild(griddiffge.domNode);
    dojo.byId("gridSurv").appendChild(gridsurv.domNode);
    //dojo.byId("gridcmpcolors").appendChild(gridcmpcolors.domNode);
    dojo.byId("gridcmpres").appendChild(gridcmpres.domNode);
    /*Call startup() to render the grid*/
    grid.startup();
    gridcn.startup();
    gridmut.startup();
    gridcngeall.startup();
    gridcngeselect.startup();
    griddiffge.startup();
    gridsurv.startup();
    //gridcmpcolors.startup();
    gridcmpres.startup();
	document.getElementById('gridDiv').style.visibility = 'hidden';
	document.getElementById('showGEresults').style.visibility = 'hidden';
	document.getElementById('showNetworkresults').style.visibility = 'hidden';
	document.getElementById('showSurvivalresults').style.visibility = 'hidden';
	document.getElementById('showGSEAresults').style.visibility = 'hidden';
	document.getElementById('showDRIresults').style.visibility = 'hidden';
	document.getElementById('showWGCNAresults').style.visibility = 'hidden';
	document.getElementById('showCNresults').style.visibility = 'hidden';
        document.getElementById('showMutationResults').style.visibility = 'hidden';
	document.getElementById('showGemiNiresults').style.visibility = 'hidden';
	document.getElementById('pwselectDiv').style.visibility = 'hidden';
        document.getElementById('survpwselectDiv').style.visibility = 'hidden';
	document.getElementById('showCompare').style.visibility = 'hidden';
	var lastsData = [
	  {id:0, name:"None"}
	];
	lastsstore = new dojo.store.Memory({data: lastsData});
	/*Call pathways */
	var xhrArgs = {
		url: "get_pathways.php",
		handleAs: "text",
		load: function(data){
			var myStore = new dojo.data.ItemFileReadStore({
				url: 'get_pathways.php'
			});

                        var myStore2 = new dojo.data.ItemFileReadStore({
                                url: 'get_comp_std.php'
                        });  
                        var myStore3 = new dojo.data.ItemFileReadStore({
                                url: 'get_comp_std_surv.php'
                        });	
		
			var filteringSelect = new dijit.form.FilteringSelect({
				id: "pathwaySelectGE",
				name: "pathwaySelectGE",
				store: myStore,
				searchAttr: "name",
				required: false,
				value: "1",
				queryExpr: "*${0}*",
				autoComplete: false,
				messages: "MSigDB Pathways"
			},"pathwaySelectGE");
                        var filteringSelect2 = new dijit.form.FilteringSelect({
                                id: "pathwaySelectSurvival",
                                name: "pathwaySelectSurvival",
                                store: myStore,
                                searchAttr: "name",
                                required: false,
                                value: "1",
                                queryExpr: "*${0}*",
                                autoComplete: false,
                                messages: "MSigDB Pathways"
                        },"pathwaySelectSurvival");
                        var filteringSelect3 = new dijit.form.FilteringSelect({
                                id: "pathwaySelectComp",
                                name: "pathwaySelectComp",
                                store: myStore,
                                searchAttr: "name",
                                required: false,
                                value: "1",
                                queryExpr: "*${0}*",
                                autoComplete: false,
                                style: "width: 300px;",
                                messages: "MSigDB Pathways"
                        },"pathwaySelectComp");
                        var filteringSelect4 = new dijit.form.FilteringSelect({
                                id: "stdSelectComp",
                                name: "stdSelectComp",
                                store: myStore2,
                                searchAttr: "name",
                                required: false,
                                value: "1",
                                queryExpr: "*${0}*",
                                autoComplete: false,
				style: "width: 300px;",
                                messages: "Studies"
                        },"stdSelectComp");
                        var filteringSelect5 = new dijit.form.FilteringSelect({
                                id: "survpathwaySelectComp",
                                name: "survpathwaySelectComp",
                                store: myStore,
                                searchAttr: "name",
                                required: false,
                                value: "1",
                                queryExpr: "*${0}*",
                                autoComplete: false,
                                style: "width: 300px;",
                                messages: "MSigDB Pathways"
                        },"survpathwaySelectComp");
                        var filteringSelect6 = new dijit.form.FilteringSelect({
                                id: "survstdSelectComp",
                                name: "survstdSelectComp",
                                store: myStore3,
                                searchAttr: "name",
                                required: false,
                                value: "1",
                                queryExpr: "*${0}*",
                                autoComplete: false,
                                style: "width: 300px;",
                                messages: "Studies"
                        },"survstdSelectComp");
		},
		error: function(error){
		  alert(error);
		}
	}
	var deferred = dojo.xhrGet(xhrArgs);
	dojo.style("btnAddStd","width","295px");
	dojo.style("btnAddPw","width","295px");
        dojo.style("btnRemoveStd","width","295px");
        dojo.style("btnRemovePw","width","295px");
	dojo.style("btnCompare","width","295px");
});
 
</script>
