<script>
	$("#genes").change(function() {
	exons.innerHTML = "";
	for(var i = 2; i < $(this).val(); i++) {
		var opt = i;
		exons.innerHTML += "<option value=\"" + opt + "\">" + opt + "</option>";
	}
 	var selectedText = genes.options[genes.selectedIndex].innerHTML;
	var selectedValue = genes.value;
	document.getElementById('g').value= selectedText;

	});
 
 	$("#exons").change(function() {
	document.getElementById('ex').value= $(this).val();
	});	
</script>

