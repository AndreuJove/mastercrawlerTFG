<SCRIPT language=JavaScript1.2>

function testInfo(aForm)
{
    aForm = document.forms[0];
    re = new RegExp("[^0-9]");

    OK = re.exec(aForm.limit.value)
    if (OK)
    {
        window.alert ("Score field should be a numeric")
        aForm.limit.focus();
        return !OK
    }



    return !OK
}
</SCRIPT>

