function popwin (url,w,h,name) {
  if (w =='') w = 60;
  if (h =='') h = 2;
  name = 'popwin';
  var settings= "toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,width=" + w + ",height=" + h ;
  external = window.open(url,name,settings);
  external.focus();
}
</script>
