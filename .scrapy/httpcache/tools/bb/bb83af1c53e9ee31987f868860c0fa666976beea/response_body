<form name="prompt" method="get" action='/Tools/dbfetch/dbfetch'
	  enctype="application/x-www-form-urlencoded" style="margin-bottom:1em; margin-top:0px"
	  onsubmit="if (document.prompt.id.value=='') {alert('Please enter your search items.');
	  document.prompt.id.focus(); document.prompt.id.select(); return; }">
	<div class="xround"><b class="xtop"><b class="xb1"></b><b class="xb2"></b><b class="xb3"></b>
		<b class="xb4"></b></b><div class="xboxcontent">
		<table style="width:90%; margin: 0px auto;" class="inputform" summary="options">
			<tr style="text-align:center;">
				
				<td style="width:30%;"><a href="dbfetch/dbfetch.databases">Database</a></td>
				<td style="width:30%;"><a href="#SearchItems">Search Items</a></td>
				<td style="width:15%"><a href="#Format">Format</a></td>
				<td style="width:15%;"><a href="#OutputStyle">Style</a></td>
				<td style="width:10%;"></td>
			</tr>
			<tr style="text-align:center;">
				<td style="width:30%">
					<select name="db" id="dbs" style="width: 98%;" OnChange="dbSelected();">
					<!-- Generate the database drop-down select form. Important that selectedDB is set in the request (in the Servlet). -->
					
						
							
