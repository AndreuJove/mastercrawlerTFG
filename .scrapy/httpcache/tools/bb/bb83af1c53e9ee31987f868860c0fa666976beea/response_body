<script language="javascript" type="text/javascript">

function callLoadMethods() {
	if(document.prompt) {
		loadForm();
	}
	dbSelected();
}

function loadForm() {
	var formName="";
	var dbName="";
	var formatName="";
	var styleName="";
	var urlArray = new Array();
	urlArray= document.location.href.split("?");
	if(urlArray[1]) {
		var urlArray2 = new Array();
		tmp = urlArray[1];
		urlArray2= tmp.split("&");
		for(var i=0; i<urlArray2.length; i++) {
			var tmpArray = new Array();
			tmpArray = urlArray2[i].split("=");
			if(i == 0) {
				if(tmpArray[0] == "db") {
					formName = "prompt";
					dbName = tmpArray[1];
				}
				else {
					formName = "prompt2";
				}
			}
			if(tmpArray[0] == "format") {
				formatName = tmpArray[1];
			}
			if(tmpArray[0] == "style") {
				styleName = tmpArray[1];
			}
		}
		if(formName == "prompt" && dbName != ""  && formatName !="" && styleName !="") {
			setDSBFetchSelect(document.prompt.db, dbName);
			setDSBFetchSelect(document.prompt.format, formatName);
			setDSBFetchSelect(document.prompt.style, styleName);
		}
		else if(formName == "prompt2" && formatName !="" && styleName !="") {
			setDSBFetchSelect(document.prompt2.format, formatName);
			setDSBFetchSelect(document.prompt2.style, styleName);
			//document.prompt2.
		}
		else{}
	}
}

function setDSBFetchSelect(selDropDown, selValue) {
	if(selValue) {
		selValue = selValue.replace(/%20/g, " ");
		for(var i=0; i< selDropDown.options.length; i++) {
			if(selDropDown.options[i].value == selValue) {
				for(var x=0; x< selDropDown.options.length; x++) {
						selDropDown.options[x].selected=false;
				}
				selDropDown.options[i].selected=true;
				break;
			}
			else{}
		}
	}
	else{}
}

function dbSelected() {
	var selectedIndex = document.prompt.db.selectedIndex;
	var dbSel = document.getElementById("dbs");
	var formatSel = document.getElementById("formats");
	var styleSel = document.getElementById("styles");

	// Empty Options array for format.
	for(var i=0; i<formatSel.options.length; i++) {
		formatSel.options[i] = null;
	}
	// Assign the supported formats.
	var selDbName = dbSel.options[dbSel.selectedIndex].value;
	for(var i=0; i < formatHash[selDbName].length; i++) {
		if(formatHash[selDbName][i] == 'default') {
			formatSel.options[i] = new Option(formatHash[selDbName][i], formatHash[selDbName][i], true, true);
		}
		else {
			formatSel.options[i] = new Option(formatHash[selDbName][i], formatHash[selDbName][i], false, false);
		}
	}
	// Update the styles select drop-down
	formatSelected();
}

// Update the content of the styles select drop-down
function formatSelected(){
    var formatElem = document.getElementById("formats");
    var styleElem = document.getElementById("styles");
    var dbElem = document.getElementById("dbs");
    var selectedFormat = formatElem.options[formatElem.selectedIndex].value;
    var selectedDb = dbElem.options[dbElem.selectedIndex].value;

    // Empty Styles array.
	for(var i=0; i<styleElem.options.length; i++) {
		styleElem.options[i] = null;
	}

	// Assign the supported styles.
	for(var i=0; i < styleHash[selectedDb][selectedFormat].length; i++) {
		if(styleHash[selectedDb][selectedFormat][i] == 'default') {
			styleElem.options[i] = new Option(styleHash[selectedDb][selectedFormat][i], styleHash[selectedDb][selectedFormat][i], true, true);
		}
		else {
			styleElem.options[i] = new Option(styleHash[selectedDb][selectedFormat][i], styleHash[selectedDb][selectedFormat][i], false, false);
		}
	}
}
<!-- Code which dynamically generates styleHash and formatHash JavaScript variables -->
