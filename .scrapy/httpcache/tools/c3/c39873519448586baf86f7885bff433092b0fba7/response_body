    var fn=document.getElementById('id_email').value;
    var filter=/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i ;
    var my_pree	;


//if ((x == null || x == "") && (y == null || y == "")  && (z == null || z == "") && (w == null || w == "")) {
  //      box=bootbox.alert("Please fill in the required information according to the input type");
//	box.find(".btn-primary").removeClass("btn-primary").addClass("btn-info")

  //      return false;
   // } 

if ( x !="" && w ==""){
   	box=bootbox.alert("First chain will be taken as no chain was given as input");
        box.find(".btn-primary").removeClass("btn-primary").addClass("btn-info");
  //    return false;
    }

if ( (x== "") && (y=="") && (w=="") && (z !="" || z !=null) ){

    var n = z.indexOf(">");
    var hola =z.length;
//    window.alert(hola);
    if (hola > "1") {
    if (n != "0") {
   
   // window.alert(z.length);
    box=bootbox.alert("Please submit the sequence in FASTA format");
    box.find(".btn-primary").removeClass("btn-primary").addClass("btn-info");

    return false;
    }
	}
   else {
	box=bootbox.alert("Please fill in the required information according to the input type ");
	box.find(".btn-primary").removeClass("btn-primary").addClass("btn-info");
	return false;

}
}

 
