<script type="text/javascript">



function showme(mydata)
{
 document.getElementById(mydata).disabled = '';
 document.getElementById(mydata).style.display = 'inline';

}


function Hideme(myData)
{
 document.getElementById(myData).disabled = 'disabled';


}



function resetform(myData) {
document.getElementById(myData).value="";
}

function checkmail() {
 var fn=document.getElementById('id_email').value;
    if(fn == ""){
        alert('Please Enter Valid Email');
	
	document.getElementById('id_email').style.border = "thick dotted red";
//         document.getElementById('id_email').style.borderWidth = "thick";
        return false;
}
}

function resetmail() {
document.getElementById('id_email').style.border = document.getElementById('id_job_name').style.border;
//document.getElementById('id_email').style.border = "medium solid grey";
}
function ResetForm()
 {
document.getElementById('id_job_name').value="";
document.getElementById('id_pdb_name').value="";
document.getElementById('id_chain_name').value="";
document.getElementById('id_seq_name').value="";
document.getElementById('id_stru_name').value="";
document.getElementById('id_pdb_name').disabled = '';
document.getElementById('id_pdb_name').style.display = 'inline';
document.getElementById('id_chain_name').disabled = '';
document.getElementById('id_chain_name').style.display = 'inline';
document.getElementById('id_stru_name').disabled = '';
document.getElementById('id_stru_name').style.display = 'inline';
document.getElementById('id_seq_name').disabled = 'disabled';

 }
 
 
function resetall()
{
document.getElementById('id_pdb_name').value="";
document.getElementById('id_chain_name').value="";
document.getElementById('id_seq_name').value="";
document.getElementById('id_stru_name').value="";
document.getElementById('id_job_name').value="";

}

function validateForm() {
    var x = document.forms["my_job"]["job_name"].value;
    if (x == null || x == "") {
        box=bootbox.alert("Please enter a valid Job ID");
	box.find(".btn-primary").removeClass("btn-primary").addClass("btn-info")
        return false;
    }
}



