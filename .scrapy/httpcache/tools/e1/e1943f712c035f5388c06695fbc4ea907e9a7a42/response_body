  <script>
     var drug=[
     ["All trans retinoic acid (ATRA)","Anti-benzo[a]pyrene-trans-7,8-diol-9,10-epoxide (anti-BPDE)","Arsenic","Artesunate","Benzo[a]pyrene","Bisphenol-A (BPA)","Bleomycin (BLM)","Botulinum Toxin Type A (BoNTA)","Cadmium","Calycosin","Carboplatin","Carboplatin + Docetaxel","Cisplatin","Clopidogrel","Curcumin","Cycloheximide","Dexamethasone","Dextrose, unspecified form","Diamorphine","Diethylstilbestrol (DES)","Dioscin","Docetaxel","Doxorubicin","Fentanyl","Fluorouracil","Fucoxanthin","Fucoxanthin + Palmitic acid","Genistein","Hydrogen peroxide","Imatinib","Isoprenaline","Lipopolysaccharide","Metformin","Methotrexate (MTX)","Methylprednisolone","Oxaliplatin","Paclitaxel (PTX)","Palmitic Acid","Palmitic acid","Panobinostat","Phenformin","Platinum","Propofol","Proteasome inhibitors","Quercetin","Simvastatin","Sorafenib","Tamoxifen","Trichostatin A","beta-Asarone"],
     ["2,3,7,8-tetrachlorodibenzo-p-dioxin (TCDD)","Bisphenol-A (BPA)","Bleomycin (BLM)","Cyclophosphamide","Diethyl 1,4-dihydro-2,4,6,-trimethyl-3,5-pyridinedicarboxylate (DDC)","Folic Acid (FA)","Furan","High glucose (HG)","Ketamine","Metamfetamine","Salvianolic acid B (Sal B)","beta-Asarone"],
     ["Berberine","Losartan"],
     ["Berberine","Losartan"],
     ["beta-diketone antibiotic (DKAs)"],
     ["Bisphenol-A (BPA)"]
     ];
     function getDrug(){
         //获得物种下拉框的对象
         var sltSpecies1=document.search1.species1;
         //获得药物下拉框的对象
         var sltDrug=document.search1.drug1;
         //得到对应物种的药物数组
         var species1Drug=drug[sltSpecies1.selectedIndex - 1];
         //清空药物下拉框，仅留提示选项
         sltDrug.length=1;
 
         //将药物数组中的值填充到物种下拉框中
         for(var i=0;i<species1Drug.length;i++){
            sltDrug[i+1]=new Option(species1Drug[i],species1Drug[i]);
         }
         
     }
  </script>
  <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
  <script>
      var data1;  
    //读取本地json 文件  
      $.ajax({  
          cache: false,  
          async: false,// 通过请求 返回函数将得到的data 赋值给外部变量  
          type: 'post',          
          url: "json/validated_lnc.json",  
          success: function (data) {  
              data1 =eval(data);  
          }  
      });
    $( function () {
    $.widget( "custom.catcomplete", $.ui.autocomplete, {
      _create: function() {
        this._super();
        this.widget().menu( "option", "items", "> :not(.ui-autocomplete-category)" );
      },
      _renderMenu: function( ul, items ) {
        var that = this,
          currentCategory = "";
        $.each( items, function( index, item ) {
          var li;
          if ( item.category != currentCategory ) {
            ul.append( "<li class='ui-autocomplete-category'>" + item.category + "</li>" );
            currentCategory = item.category;
          }
          li = that._renderItemData( ul, item );
          if ( item.category ) {
            li.attr( "aria-label", item.category + " : " + item.label );
          }
        });
      }
    });
    $( "#lncrna1" ).catcomplete({
      delay: 0,
      source: data1
    });
  } );
  </script>    
  <script>
      var data2;  
    //读取本地json 文件  
      $.ajax({  
          cache: false,  
          async: false,// 通过请求 返回函数将得到的data 赋值给外部变量  
          type: 'post',          
          url: "json/cmap.json",  
          success: function (data) {  
              data2 =eval(data);  
          }  
      });  
      $.ajax({  
          cache: false,  
          async: false,// 通过请求 返回函数将得到的data 赋值给外部变量  
          type: 'post',          
          url: "json/geo.json",  
          success: function (data) {  
              data3 =eval(data);  
          }  
      }); 
      function show_sub(v){
        $.widget( "custom.catcomplete", $.ui.autocomplete, {
      _create: function() {
        this._super();
        this.widget().menu( "option", "items", "> :not(.ui-autocomplete-category)" );
      },
      _renderMenu: function( ul, items ) {
        var that = this,
          currentCategory = "";
        $.each( items, function( index, item ) {
          var li;
          if ( item.category != currentCategory ) {
            ul.append( "<li class='ui-autocomplete-category'>" + item.category + "</li>" );
            currentCategory = item.category;
          }
          li = that._renderItemData( ul, item );
          if ( item.category ) {
            li.attr( "aria-label", item.category + " : " + item.label );
          }
        });
      }
    });
    if(v=="cMap"){
      $( "#lncrna2" ).catcomplete({
      delay: 0,
      source: data2
    });  
    }if(v==="GEO"){
      $( "#lncrna2" ).catcomplete({
      delay: 0,
      source: data3
    });  
    }
    }
  </script>
