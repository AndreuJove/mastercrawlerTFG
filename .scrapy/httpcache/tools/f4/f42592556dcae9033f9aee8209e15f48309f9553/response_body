	function checkForm2(n) {
		j=0;
		for (i=0; i<n; i++) {
			if (document.getElementById("cb" + i).checked) {
				j++;
			}
		}
		if(j == 0) {
			alert("Please choose at least one template.");
			return false;
		}
	}
