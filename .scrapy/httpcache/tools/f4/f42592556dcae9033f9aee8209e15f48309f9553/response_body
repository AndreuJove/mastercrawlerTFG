<script language="JavaScript" type="text/javascript">
	function checkForm() {
		var email=document.getElementById("address").value;
		if(email.length!=0) {
			if( email.lastIndexOf("@") == -1 ||
				email.lastIndexOf(".") < email.lastIndexOf("@") ) {
				alert("Please enter a correct e-mail address.");
				document.getElementById("address").focus();
				document.getElementById("address").select();
				return false;
			}
		}
		
		var targetName=document.getElementById("target").value;
		if( targetName.length == 0 ) {
			alert( "Please enter a target name." );
			document.getElementById("target").focus();
			document.getElementById("target").select();
			return false;
		}
		
		var seq=document.getElementById("seq").value;
		var seq_up=document.getElementById("seq_up").value;
		if( (seq.length < 30 || seq.length > 1250) && seq_up.length == 0) {
			alert( "Please enter a query sequence. (30 < residue number < 1200)" );
			document.getElementById("seq").focus();
			document.getElementById("seq").select();
			return false;
		}
		
		var seq=document.getElementById("seq").value;
		num = 0;
		for(i=0;i<seq.length;i++) {
			if(seq.charAt(i)=="A" || seq.charAt(i)=="T" || seq.charAt(i)=="C" || seq.charAt(i)=="G" 
			|| seq.charAt(i)=="a" || seq.charAt(i)=="t" || seq.charAt(i)=="c" || seq.charAt(i)=="g")
			num++;
		}
		if( num / (seq.length-20) > 0.75) {
			alert( "Please enter a protein sequence not DNA sequence." );
			document.getElementById("seq").focus();
			document.getElementById("seq").select();
			return false;
		}
		
		if(document.getElementById("r2").checked) {
			var pdbId=document.getElementById("pdbId").value;
			if( pdbId.length == 0 || pdbId.length != 4) {
				alert( "Please enter a correct PDB code." );
				document.getElementById("pdbId").focus();
				document.getElementById("pdbId").select();
				return false;
			}
			var chain=document.getElementById("chain").value;
			if( chain.length != 0 && chain.length != 1) {
				alert( "Please enter a correct protein Chain ID." );
				document.getElementById("chain").focus();
				document.getElementById("chain").select();
				return false;
			}
		}
	}
	

	function checkForm_2() {
/*
		var list = JavaPowUpload.getFiles();


		var txtFile = "temp\/20200331_ghZO6aYz3yW0xkLVkpsC";
//		alert(txtFile);

		if(list.size() >= 1) {
			for(i=list.size()-1;i>=0;i--) {
 				if(list.get(i).isFile() && list.get(i).getStatus() == 2) {
//  					alert(list.get(i).getFullPath()); 
//					alert(list.get(i).getFileName());
				}
			}
		} else {
			alert("Please upload sequence files.");
			return false;
		}
*/
		var email=document.getElementById("address").value;
		if(email.length!=0) {
			if( email.lastIndexOf("@") == -1 ||
				email.lastIndexOf(".") < email.lastIndexOf("@") ) {
				alert("Please enter a correct e-mail address.");
				document.getElementById("address").focus();
				document.getElementById("address").select();
				return false;
			}
		}
		
		var databaseID=document.getElementById("profileId").value;
		if( document.getElementById("r2").checked && databaseID.length == 0 ) {
			alert( "Please enter a database ID." );
			document.getElementById("profileId").focus();
			document.getElementById("profileId").select();
			return false;
		}

		var targetName=document.getElementById("target").value;
		if( targetName.length == 0 ) {
			alert( "Please enter a target name." );
			document.getElementById("target").focus();
			document.getElementById("target").select();
			return false;
		}
		
		var userScheme=document.getElementById("sch2").value;
		if( document.getElementById("r2").checked && userScheme.length == 0) {
			alert( "Please enter user defined scheme." );
			document.getElementById("sch2").focus();
			document.getElementById("sch2").select();
			return false;
		}

		var scheme_up=document.getElementById("scheme_up").value;
		if( document.getElementById("r3").checked && scheme_up.length == 0) {
			alert( "Please upload an user defined scheme file." );
			document.getElementById("scheme_up").focus();
			document.getElementById("scheme_up").select();
			return false;
		}

	}

	function checkForm_3() {
		var email=document.getElementById("address").value;
		if(email.length!=0) {
			if( email.lastIndexOf("@") == -1 ||
				email.lastIndexOf(".") < email.lastIndexOf("@") ) {
				alert("Please enter a correct e-mail address.");
				document.getElementById("address").focus();
				document.getElementById("address").select();
				return false;
			}
		}

		var scheme_up=document.getElementById("scheme_up").value;
		if( scheme_up.length == 0) {
			alert( "Please upload an user download scheme file." );
			document.getElementById("scheme_up").focus();
			document.getElementById("scheme_up").select();
			return false;
		}

	}


	function disable(n) {
		var obj = document.getElementById(n);
		obj.disabled = true;
	}

	function enable(n) {
		var obj = document.getElementById(n);
		obj.disabled = false;
	}
	function focuson(n) {
		document.getElementById(n).focus();
		document.getElementById(n).select();
	}
	
	function disfocus(n) {
		document.getElementById(n).blur();
	}
	
	function resetText(n) {
		var obj = document.getElementById(n);
		if(obj.value=="T0300" || obj.value=="MASKKPDKTYEEMVKEVERLKLENKTLKQKVKSSGAVSSDDSILTAAKRESIIVSSSRALGAVAMRKIEAKVRSRAAKAVTEQELTSLLQSLTLRVDVSMEE")
			obj.value = "";
	}
