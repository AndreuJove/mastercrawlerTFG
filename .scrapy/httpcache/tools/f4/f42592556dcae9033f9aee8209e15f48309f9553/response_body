<script language="javascript">
	function tableLight($i) {
		qxt.rows[$i+1].style.backgroundColor="#DAE4EF";
	}
	function tableOff($i) {
		qxt.rows[$i+1].style.backgroundColor="";
	}

	function choose(n) {
		j=0;
		for (i=0; i<n; i++) {
			if (document.getElementById("cb" + i).checked) {
				j++;
			}
		}
		if (j>=5) { 
			for (i=0; i<n; i++) {
				if (!(document.getElementById("cb" + i).checked)) {
					document.getElementById("cb" + i).disabled = true;
				}
			}
		} else {
			for (i=0; i<n; i++) {
				document.getElementById("cb" + i).disabled = false;
			}
		}
	}
	
	function selectAll(m) { 
	//	var theForm = document.formCMP;
		var obj=document.getElementById("checkAll"); 
		if(obj.checked) { 
		   	for (i=1; i<=m; i++) {
		   		document.getElementById(i).checked = true;
			}
		}
	} 

	function select95(m,n) { 
	//	var theForm = document.formCMP;
		var obj=document.getElementById("check95"); 
		if(obj.checked) { 
			for (i=1; i<=m; i++) {
				document.getElementById(i).checked = false;
			}
		   	for (i=1; i<=n; i++) {
		   		document.getElementById(i).checked = true;
			}
		}
	} 

