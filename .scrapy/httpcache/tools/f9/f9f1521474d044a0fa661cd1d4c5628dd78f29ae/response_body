    <script>
        
        window.onload = function() {
            var userAgent=navigator.userAgent.toLowerCase(), s, o = {};
//            alert(userAgent)
            /* get the browser's name and version*/
            /*var browser={
                version:(userAgent.match(/(?:firefox|opera|safari|chrome|msie)[\/: ]([\d.]+)/))[1],
                safari:/version.+safari/.test(userAgent),
                chrome:/chrome/.test(userAgent),
                firefox:/firefox/.test(userAgent),
                ie:/msie/.test(userAgent),
                opera: /opera/.test(userAgent )
            }*/
            var browser = {
                safari: userAgent.indexOf('safari') != -1,
                chrome: userAgent.indexOf('chrome') != -1,
                firefox: userAgent.indexOf('firefox') != -1,
                wechat: userAgent.toLowerCase().indexOf('micromessenger') != -1
            }
//            alert(browser.chrome)
//            alert(browser.firefox)
//            alert(browser.safari)

            if (! (browser.safari || browser.chrome || browser.firefox || browser.wechat)) {
                alert("We are sorry but this website doesn't support your browser!")
                var para=document.createElement("p");
                var text=document.createTextNode("We are sorry, but this website doesn't support your browser! Please use the latest version of Firefox, Chrome or Safari to browse AnnoLnc.");
                para.appendChild(text)
                para.style.color = '#CC625D'
                para.style.fontSize = '25px'
                var warn = document.getElementById('al-browser')
                warn.appendChild(para)
//                $('#al-browser').append("<p>We are sorry, but this website doesn't support your browser! Please use the latest version of Firefox, Chrome or Safari to browse AnnoLnc.</p>")
                warn.style.textAlign ='center'
                warn.style.marginTop ='20px'

                 /*$('#al-browser').css({
                 'text-align': 'center',
                 'margin-top': '20px'
                 })
                 $('#al-browser p').css({
                 'color': '#CC625D',
                 'font-size': '25px'
                 })*/
            }
        }

        $(document).ready(function(){

            
            $('#demo-seq li').click(function(){
                var name = $(this).find('a').text()
                var url = '/seq_tryout/' + name + '.fa'
                $.get(url, function(data, status){
                    $('#input-seq').val(data)
                })
//                $('#input-seq').load('/seq_tryout/' + fileName + '.fa')
            })

/*            $('#demo-search li').click(function(){
                var demo = $(this).find('a').text()
                $('#input-search').val(demo)
            })*/

//            $('#input-seq .placeholder').css("font-size","20px")
        })
    </script>
