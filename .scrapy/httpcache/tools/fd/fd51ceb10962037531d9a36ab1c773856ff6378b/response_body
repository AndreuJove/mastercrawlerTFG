<script>
    /**
     * @function onDomInsert - Listener for nodes inserted into DOM.
     *                         Does NOT monitor when things are removed.
     *                         Based on http://stackoverflow.com/a/14570614/445295
     * @param [DOMNode]  el  - Optional. The element to listen on. Defaults to document.
     * @param [Function] cb  - Callback. Called with inserted element as only arg.
     */
    var onDomInsert = (function(){
        'use strict';
        
        var MutationObserver = window.MutationObserver || window.WebKitMutationObserver;

        // For Chrome 18+, Firefox 14+, Safari 6+, IE 11+.
        if( MutationObserver ){
            
            return function( el, callback ){
                if( !callback ){ callback = el; el = document; }
                (new MutationObserver(function(mutations){
                    for( var i=0; i<mutations[0].addedNodes.length; i++ ){
                        callback( mutations[0].addedNodes[i] );
                    }
                })).observe( el, { childList:true, subtree:true });
            };
        }

        // legacy support DOMNodeInserted - older browsers
        if( document.addEventListener ){
            return function( el, callback ){
                if( !callback ){ callback = el; el = document; }
                el.addEventListener( 'DOMNodeInserted', function( evt ){
                    callback( evt.target );
                }, false );
            };
        }
    })();

    // begin listener
    onDomInsert( document.body, function(el) {
    
        // if the cybba form is injected, listen for the form click to perform the additional impact radius tracking event
        if (el.className == 'addshoppers_b_modal') {
            window.cybbaEmail = '';
            $('input[name=email]').change(function() {
              window.cybbaEmail = $('input[name=email]').val();
              console.log(window.cybbaEmail)
            });
        
            var cybbaFormSubmitButton = $('#hide');
            cybbaFormSubmitButton.unbind('click');
            cybbaFormSubmitButton.click(function() {
              
                // required advertiser supplied values, get encoded email from service to set as order id
                $.getJSON( "https://api.buydomains.com/affiliate/encode/" + window.cybbaEmail, function( data ) {
                      (function(d, script) {
                        script = d.createElement('script');
                        script.type = 'text/javascript';
                        script.async = true;
                        script.onload = function(){
                            console.log("*** Impact Radius ***");
                            console.log(data.encoded);
                            irEvent.setOrderId(data.encoded);
                            irEvent.fire();
                        };
                        script.src = 'https://d33wwcok8lortz.cloudfront.net/js/3298/17327/irv3.js';
                        d.getElementsByTagName('head')[0].appendChild(script);
                    }(document));
                });
            });
        }
    } );
</script>
