ect();\n\t\t\treturn false;\n\t\t}\n\t\t\n\t\tvar seq=document.getElementById(\"seq\").value;\n\t\tif( seq.length < 20 || seq.length > 2100) {\n\t\t\talert( \"Please enter a query sequence. (20 < residue number < 2000)\" );\n\t\t\tdocument.getElementById(\"seq\").focus();\n\t\t\tdocument.getElementById(\"seq\").select();\n\t\t\treturn false;\n\t\t}\n\t\t\n\t\tvar seq=document.getElementById(\"seq\").value;\n\t\tnum = 0;\n\t\tfor(i=0;i<seq.length;i++) {\n\t\t\tif(seq.charAt(i)==\"A\" || seq.charAt(i)==\"T\" || seq.charAt(i)==\"C\" || seq.charAt(i)==\"G\" \n\t\t\t|| seq.charAt(i)==\"a\" || seq.charAt(i)==\"t\" || seq.charAt(i)==\"c\" || seq.charAt(i)==\"g\")\n\t\t\tnum++;\n\t\t}\n\t\tif( num / (seq.length-20) > 0.75) {\n\t\t\talert( \"Please enter a protein sequence not DNA sequence.\" );\n\t\t\tdocument.getElementById(\"seq\").focus();\n\t\t\tdocument.getElementById(\"seq\").select();\n\t\t\treturn false;\n\t\t}\n\t\t\n\t\tif(document.getElementById(\"r3\").checked) {\n\t\t\tvar pdbId=document.getElementById(\"pdbId\").value;\n\t\t\tif( pdbId.length == 0 || (pdbId.length != 4 && pdbId.length != 6) || pdbId == \"xxxx_y\" ) {\n\t\t\t\talert( \"Please enter a correct PDB code.\" );\n\t\t\t\tdocument.getElementById(\"pdbId\").focus();\n\t\t\t\tdocument.getElementById(\"pdbId\").select();\n\t\t\t\treturn false;\n\t\t\t}\n\t\t}\n\t}\n\t\n\tfunction disable(n) {\n\t\tvar obj = document.getElementById(n);\n\t\tobj.disabled = true;\n\t}\n\n\tfunction enable(n) {\n\t\tvar obj = document.getElementById(n);\n\t\tobj.disabled = false;\n\t}\n\t\n\tfunction focuson(n) {\n\t\tdocument.getElementById(n).focus();\n\t\tdocument.getElementById(n).select();\n\t}\n\t\n\tfunction disfocus(n) {\n\t\tdocument.getElementById(n).blur();\n\t}\n\t\n\tfunction resetText(n) {\n\t\tvar obj = document.getElementById(n);\n\t\tif(obj.value==\"T0300\" || obj.value==\"MASKKPDKTYEEMVKEVERLKLENKTLKQKVKSSGAVSSDDSILTAAKRESIIVSSSRALGAVAMRKIEAKVRSRAAKAVTEQELTSLLQSLTLRVDVSMEE\")\n\t\t\tobj.value = \"\";\n\t}\n